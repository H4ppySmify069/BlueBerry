local ExampleToggle11 = Groupbox4:CreateToggle("Enabled", function(value)
    enabled1 = value
end)

local ExampleToggle12 = Groupbox4:CreateToggle("No Recoil", function(value)
    for i,v in pairs (game:GetService("ReplicatedStorage").Weapons:GetDescendants()) do 
        if v and v.Name ~= "C4" and v:FindFirstChild("Recoil") then 
           v:FindFirstChild("Recoil").Value = 0
        v:FindFirstChild("Recoil").AngleY.Value = 0
        v:FindFirstChild("Recoil").AngleX.Value = 0
        for i,v in next,debug.getregistry() do
         if type(v) =='function'then
          local i = debug.getupvalues(v)
          debug.setupvalue(v,'RecoilX',0)
          debug.setupvalue(v,'RecoilY',0)
          end
          end
          end
          end
        end)

local ExampleToggle13 = Groupbox4:CreateToggle("No Spread", function(value)
    for i,v in pairs (game:GetService("ReplicatedStorage").Weapons:GetDescendants()) do
        if v and v.Name ~= "C4" and v:FindFirstChild("Spread") then
           v:FindFirstChild("Spread").Value = 0
           v:FindFirstChild("Spread").Stand.Value = 0
           v:FindFirstChild("Spread").MaxInaccuracy.Value = 0
           v:FindFirstChild("Spread").Crouch.Value = 0
           v:FindFirstChild("Spread").Move.Value = 0
           v:FindFirstChild("Spread").Jump.Value = 0
           v:FindFirstChild("Spread").Ladder.Value = 0
           v:FindFirstChild("Spread").RecoveryTime.Value = 0
           v:FindFirstChild("Spread").RecoveryTime.Crouched.Value = 0
        end
        end
        end)
local ExampleToggle14 = Groupbox4:CreateToggle("Inf Ammo", function(value)
    for i,v in pairs (game:GetService("ReplicatedStorage").Weapons:GetDescendants()) do 
        if v and v.Name ~= "C4" and v:FindFirstChild("Ammo") then 
           v:FindFirstChild("Ammo").Value = 9999
        v:FindFirstChild("StoredAmmo").Value = 9999
        end
        end
        end)

        local lp = game:GetService("Players").LocalPlayer
        local client = getsenv(lp.PlayerGui.Client)
        local spincurrent = 0
        local hitpart = nil
        game:GetService('RunService').RenderStepped:Connect(function()
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.Space) and game:GetService("UserInputService"):GetFocusedTextBox() == nil then
                if lp.Character and lp.Character:FindFirstChild('Humanoid') and lp.Character:FindFirstChild('HumanoidRootPart') then
                    if lp.Character.HumanoidRootPart:FindFirstChild('gamervelocity') then
                        lp.Character.HumanoidRootPart.gamervelocity.Velocity = (lp.Character.Humanoid.MoveDirection) * bhopspeed
                        lp.Character.Humanoid.Jump = true
                    else
                        lp.Character.Humanoid.Jump = true
                        local velocity = Instance.new('BodyVelocity', lp.Character.HumanoidRootPart)
                        velocity.Name = 'gamervelocity'
                        velocity.maxForce = Vector3.new(100000, 0, 100000)
                        velocity.Velocity = (lp.Character.Humanoid.MoveDirection) * bhopspeed
                    end
                end
            else
                if lp.Character and lp.Character:FindFirstChild('Humanoid') and lp.Character:FindFirstChild('HumanoidRootPart') then
                    if lp.Character.HumanoidRootPart:FindFirstChild('gamervelocity') then
                        lp.Character.HumanoidRootPart.gamervelocity:Destroy()
                    end
                end
            end
        end)

