local LocalPlayer = game.Players.LocalPlayer
_G.ChamsEnabled = false
_G.ChamsColor = Color3.new(1, 0, 0)
_G.ChamsTransparency = 0.3
spawn(function()
while true do
    wait(0.1)
    
    spawn(function()
        for i,v in pairs(game.Players:GetPlayers()) do
            if v ~= LocalPlayer and v.Status.Alive.Value == true and v.Status.Team.Value ~= LocalPlayer.Status.Team.Value then
                for i2,v2 in pairs(v.Character:GetChildren()) do
             
                    if v2:IsA("BasePart") and v2 ~= v.Character.PrimaryPart then
                        if v2:FindFirstChild("Chams") then
                            v2.Chams:Destroy()
                        end
                        if _G.ChamsEnabled then
                        local BoxHandleAdornment = Instance.new("BoxHandleAdornment")
                        BoxHandleAdornment.Name = "Chams"
                        BoxHandleAdornment.Adornee = v2
                        BoxHandleAdornment.Size = v2.Size
                        BoxHandleAdornment.Color3 = _G.ChamsColor
                        BoxHandleAdornment.AlwaysOnTop = true
                        BoxHandleAdornment.ZIndex = 0
                        BoxHandleAdornment.Parent = v2
                        BoxHandleAdornment.Transparency = 0.3
                        end
                    end
                end
            end
        end
    end)
end
end)

